{
  "name": "@overlayed2/runtime-electron",
  "private": true,
  "version": "1.0.0",
  "description": "the electron runtime for overlayed2",
  "keywords": [
    "overlayed",
    "overlayed2",
    "runtime"
  ],
  "author": "bengreenier <ben@bengreenier.com>",
  "homepage": "https://overlayed.app",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "watch": "concurrently \"npm:watch-*\"",
    "watch-process": "tsc-watch --onSuccess \"electron .\" -p .",
    "watch-browser": "parcel watch src/app.html --public-url ../dist/ --hmr-hostname localhost",
    "build": "tsc -p . && parcel build src/app.html --public-url ../dist/",
    "prepublishOnly": "npm run build",
    "test": "jest --coverage --passWithNoTests",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "@types/react-test-renderer": "^16.9.1",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "concurrently": "^5.0.0",
    "electron": "^7.1.2",
    "eslint": "^6.7.2",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^24.9.0",
    "parcel": "^2.8.2",
    "react-test-renderer": "^16.12.0",
    "ts-jest": "^24.2.0",
    "tsc-watch": "^4.0.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "preset": "ts-jest",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "testRegex": [
      "(/__tests__/.*|(\\.|/)spec)\\.ts[x]?$"
    ]
  },
  "dependencies": {
    "@overlayed2/ux-core": "file:../ux-core",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  }
}
